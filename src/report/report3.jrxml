	<queryString>
		<![CDATA[SELECT
     belibarang.`kodebeli` AS belibarang_kodebeli,
     belibarang.`kodestock` AS belibarang_kodestock,
     belibarang.`kodesupplier` AS belibarang_kodesupplier,
